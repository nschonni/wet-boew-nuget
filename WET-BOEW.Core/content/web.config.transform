<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<httpProtocol>
			<customHeaders>
				<clear />
				<add name="X-UA-Compatible" value="IE=Edge" />
			</customHeaders>
		</httpProtocol>
		<staticContent>
			<mimeMap fileExtension=".mp4" mimeType="video/mp4" />
		</staticContent>
	</system.webServer>
</configuration>